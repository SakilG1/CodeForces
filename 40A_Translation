#include<bits/stdc++.h>
using namespace std;
int main()
{
    string str1, str2;
    cin >> str1;
    cin >> str2;

    int x=str1.length(), i, j, c=0;

    for(i=x-1, j=0; i>=0; i--, j++)
    {
        if(str1[i]==str2[j])
        {
            continue;
        }
        else{
            c++;
            break;
        }
    }

    if(c==0)
    {
        cout << "YES" << endl;
    }
    else{
        cout << "NO" << endl;
    }


    return 0;
}
