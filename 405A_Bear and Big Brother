#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a, b;
    cin >> a >> b;
 
    int i, c=0;
    for(i=1; i; i++)
    {
        if(a<=b)
        {
            c++;
        }
        else{
            break;
        }
        a *= 3;
        b *= 2;
    }
    cout << c << endl;
 
    return 0;
}
