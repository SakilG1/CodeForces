#include <bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli t;
    cin >> t;
    while(t--)
    {
        lli n;
        cin >> n;

        if(n>=1900)
        {
            cout << "Division 1" << endl;
        }
        else if(n>=1600 && n<1900)
            cout << "Division 2" << endl;
        else if(n>=1400 && n<1600)
            cout << "Division 3" << endl;
        else
            cout << "Division 4" << endl;
    }

    return 0;
}
