#include<bits/stdc++.h>
using namespace std;
int main()
{

    string s1, s2;
    cin >> s1;
    cin >> s2;

    int t = s1.length(), i;

    for(i=0; i<t; i++)
    {
        if(s1[i]!=s2[i])
        {
            cout << "1";
        }
        else if(s1[i]==s2[i]){
            cout << "0";
        }
    }
    cout << endl;


    return 0;
}
