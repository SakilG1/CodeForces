#include <bits/stdc++.h>
using namespace std;
#define lli long long int
int main()
{

    lli t;
    cin >> t;

    while (t--)
    {

        lli a, b, c, d, x = 0;
        cin >> a >> b >> c >> d;
        if (a < b)
            x++;
        if (a < c)
            x++;
        if (a < d)
            x++;

        cout << x << endl;
    }

    return 0;
}
