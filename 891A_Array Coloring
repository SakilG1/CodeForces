#include <bits/stdc++.h>
using namespace std;

#define lli long long int
#define no cout<<"NO"<<endl
#define yes cout<<"YES"<<endl
#define inkj i, j, n, m, k, x, c
#define fn for(i=0; i<n; i++)
#define arr(n) lli arr[n]; fn{cin>>arr[i];}

void crack()
{
    lli inkj;
    cin >> n;
    lli arr[n];
    c=0;
    for(i=0; i<n; i++)
    {
        cin >> arr[i];
        if(arr[i]%2!=0)
        {
            c++;
        }
    }
    if(c%2!=0)
    {
        no;
    }
    else{
        yes;
    }
}

int main()
{
    lli test_case=1;
    cin >> test_case;
    while (test_case--)
    {
        crack();
    }
    return 0;
}
