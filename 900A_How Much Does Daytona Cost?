#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define fn for(i=0; i<n; i++)
int main()
{
    lli t;
    cin >> t;

    while(t--)
    {
        lli n, k, c=0, i;
        cin >> n >> k;
        lli arr[n];

        fn{
            cin >> arr[i];
            if(arr[i]==k)
            {
                c++;
            }
        }
        if(c!=0)
        {
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }

    return 0;
}
