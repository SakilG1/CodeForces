#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin >> n;
    string str;
    cin >> str;

    int i, a=0, d=0;

    for(i=0; i<n; i++)
    {
        if(str[i]=='A')
        {
            a++;
        }
        else{
            d++;
        }
    }

    if(a==d)
    {
        cout << "Friendship" << endl;
    }
    else if(a>d)
    {
        cout << "Anton" << endl;
    }
    else{
        cout << "Danik" << endl;
    }


    return 0;
}
