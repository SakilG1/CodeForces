#include <bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli t;
    cin >> t;
    while (t--)
    {
        lli a, b;
        cin >> a >> b;
        lli c=min(a,b);
        lli d = max(a,b);
        if(c*2>=d)
        {
            cout << (c*2)*(c*2) << endl;
        }
        else{
            cout << d*d << endl;
        }
    }
    return 0;
}
