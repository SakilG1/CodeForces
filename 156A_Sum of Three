#include <bits/stdc++.h>
using namespace std;
#define lli long long int
int main()
{

    lli t;
    cin >> t;
    while (t--)
    {
        lli n;
        cin >> n;
        if(n<7 || n==9  )
        {
            cout << "NO" << endl;
        }
        else if((n-3)%3!=0)
        {
            cout << "YES" << endl;
            cout << "1 2 " << n-3 << endl;
        }
        else if((n-5)%3!=0){
            cout << "YES" << endl;
            cout << "1 4 " << n-5 << endl;
        }
    }

    return 0;
}
