#include <bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli t;
    cin >> t;
    while(t--)
    {
        char c;
        cin >> c;
        string str="codeforces";

        lli i, x=0;

        for(i=0; i<10; i++)
        {
            if(str[i]==c)
            {
                x++;
                break;
            }
        }
        if(x==0)
        {
            cout << "NO" << endl;
        }
        else{
            cout << "YES" << endl;
        }
    }
    return 0;
}
