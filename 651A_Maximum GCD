#include <bits/stdc++.h>
using namespace std;
#define lli long long int
#define fn for (i = 0; i < n; i++)
int main()
{
    lli t;
    cin >> t;
    while (t--)
    {
        lli n;
        cin >> n;
        if(n%2==0)
        {
            cout << n/2 << endl;
        }
        else{
            cout << (n-1)/2 << endl;
        }
    }

    return 0;
}
