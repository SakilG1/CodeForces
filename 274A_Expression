#include <bits/stdc++.h>
using namespace std;
#define lli long long int
#define fn for (i = 0; i < n; i++)
int main()
{
    lli a, b, c;
    cin >> a >> b >> c;
    lli ans = a+b+c;
    ans = max(ans, a*b*c);
    ans = max(ans, (a+b)*c);
    ans = max(ans, a*(b+c));

    cout << ans << endl;


    return 0;
}
