#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, h;
    cin >> n >> h;

    int arr[n];

    int i, c=0;
    for(i=0; i<n; i++)
    {
        cin >> arr[i];
        if(arr[i]<=h)
        {
            c++;
        }
        else{
            c += 2;
        }
    }

    cout << c << endl;

    return 0;
}
