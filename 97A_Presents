#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int arr[n], i, j;

    for(i=0; i<n; i++)
    {
        cin >> arr[i];

    }
    int ar[n];
    for(i=0; i<n; i++)
    {
        for(j=0; j<n; j++)
        {
            if(arr[j]==i+1)
            {
                ar[i]=j+1;
            }
        }
    }

    for(i=0; i<n; i++)
    {
        cout << ar[i] << " ";
    }
    cout << endl;


    return 0;
}
