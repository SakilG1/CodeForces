#include <bits/stdc++.h>
using namespace std;
#define lli long long int
#define fn for (i = 0; i < n; i++)
int main()
{
    lli t;
    cin >> t;
    while (t--)
    {
        lli a, b, c;
        cin >> a >> b >> c;
        if(a+b==c)
        {
            cout << "+" << endl;
        }
        else{
            cout << "-" << endl;
        }
    }

    return 0;
}
