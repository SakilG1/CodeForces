#include <bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli n, k;
    cin >> n >> k;
    lli arr[n], i, c=0;

    for(i=0; i<n; i++)
    {
        cin >> arr[i];
        if(5-arr[i]>=k)
        {
            c++;
        }
    }

    cout << c/3 << endl;


    return 0;
}
