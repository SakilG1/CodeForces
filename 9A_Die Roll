#include <bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli a, b;
    cin >> a >> b;
    lli x = 6-max(a,b)+1;
    if(x==6)
    {
        cout << "1/1" << endl;
    }
    else if(x==5)
    {
        cout << "5/6" << endl;
    }
    else if(x==4)
    {
        cout << "2/3" << endl;
    }
    else if(x==3)
    {
        cout << "1/2" << endl;
    }
    else if(x==2)
    {
        cout << "1/3" << endl;
    }
    else if(x==1)
    {
        cout << "1/6" << endl;
    }
    return 0;
}
