#include<bits/stdc++.h>
using namespace std;
#define lli long long int

int main()
{
    lli n;
    cin >> n;
    
    lli arr[14]={4, 7, 44, 47, 74, 77, 444, 447, 474, 477, 744, 747, 774, 777};

    lli c=0;
    for(int i=0; i<14; i++)
    {
        if(n%arr[i]==0)
        {
            cout << "YES" << endl;
            c++;
            break;
        }
    }

    if(c==0)
    {
        cout << "NO" << endl;
    }

    return 0;
}
