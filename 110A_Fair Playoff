#include <bits/stdc++.h>
using namespace std;

#define lli long long int
#define no cout<<"NO"<<endl
#define yes cout<<"YES"<<endl
#define inkj i, j, n, m, k, x, c
#define fn for(i=0; i<n; i++)
#define arr(n) lli arr[n]; fn{cin>>arr[i];}

void crack()
{
    lli inkj;
    n=4;
    arr(n)
    if((max(arr[0], arr[1])<min(arr[2], arr[3])) || (min(arr[0], arr[1])>max(arr[2], arr[3])))
    {
        no;
    }
    else{
        yes;
    }
}

int main()
{
    lli test_case=1;
    cin >> test_case;
    while (test_case--)
    {
        crack();
    }
    return 0;
}
